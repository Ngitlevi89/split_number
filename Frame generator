#include <iostream>
using namespace std;

int main()
{
    int num;
    char c;
    cout << "Enter a number" << endl;
    cin >> num;
    cout << "Enter a sign or a letter" << endl;
    cin >> c;
    for (int i = 1;i <= num;i++)
    {
        for (int j = 1;j <= num;j++)
        {
            if (i == 1 || j == 1 || i == num || j == num)
            {
                cout << c  ;
            }
            else {
                cout << " "  ;
            }
        }cout <<   endl;
    }

    return 0;


}
