#include <iostream>
using namespace std;

int main()
{
    int num,temp;
    cout << "Enter a number" << endl;
    cin >> num;
    for (int i = 1;i <= num;i++)
    {
     
        for (int j = 1;j <= num;j++)
        {
            if (i>=j)
            {
                cout << "*";
            }
            else
            {
                cout << "#";

            }

        }
        cout << endl;
    }

    return 0;


}
